{
  "name": "test-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run typecheck && vite build --config vite.config.ts",
    "client": "vite --config vite.config.ts",
    "server": "nodemon server/src/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "node server/src/server.js",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "dev:server": "cross-env DOTENV_CONFIG_PATH=./.env.local nodemon --exec \"node -r dotenv/config --experimental-modules --es-module-specifier-resolution=node\" server/src/server.js",
    "start:server": "cross-env DOTENV_CONFIG_PATH=./.env.local node -r dotenv/config --experimental-modules --es-module-specifier-resolution=node server/src/server.js",
    "dev:client": "vite"
  },
